config_version=5

[application]

config/name="Dungeon Strikers"
run/main_scene="res://Scenes/Menus/Start/start_scene.tscn"
config/features=PackedStringArray("4.2")

[autoload]

Globals="*res://Scenes/Game/globals.gd"
GameManager="*res://Scenes/Game/game_manager.gd"

[display]

window/size/viewport_width=2560
window/size/viewport_height=1536
window/size/window_width_override=1024
window/size/window_height_override=614
window/stretch/mode="viewport"
mouse_cursor/custom_image="res://Scenes/UI/src/cursor.png"
mouse_cursor/custom_image_hotspot=Vector2(8, 8)
mouse_cursor/tooltip_position_offset=Vector2(-8, -8)

[file_customization]

folder_colors={
"res://Scenes/Attacks/": "red",
"res://Scenes/Effects/": "yellow",
"res://Scenes/Entity/": "blue",
"res://Scenes/Game/": "orange",
"res://Scenes/Loot/": "green",
"res://Scenes/Menus/": "pink",
"res://Scenes/Style/": "teal",
"res://Scenes/UI/": "purple"
}

[filesystem]

import/blender/enabled=false

[gui]

theme/custom="res://Scenes/Style/ui_theme.tres"
theme/custom_font="res://Scenes/Style/light_pixel-7.ttf"

